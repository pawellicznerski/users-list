{
  "name": "autocomplete-component",
  "version": "0.1.0",
  "private": true,
  "homepage" : "https://pawellicznerski.github.io/users-list/",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "build-css": "^0.2.0",
    "node-sass-chokidar": "0.0.3",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
